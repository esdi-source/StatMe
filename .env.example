# StatMe Environment Configuration Example
# Copy this file to .env and fill in your values

# DEMO_MODE: Set to 'true' for local demo mode (no external connections)
# Set to 'false' for production mode with Supabase
DEMO_MODE=true

# Supabase Configuration (only needed when DEMO_MODE=false)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# OpenAI Configuration (only needed when DEMO_MODE=false)
OPENAI_API_KEY=sk-your-openai-key-here

# OpenFoodFacts API
OPENFOODFACTS_BASEURL=https://world.openfoodfacts.org/api/v2

# Edamam API (Fallback, only needed when DEMO_MODE=false)
EDAMAM_APP_ID=your-app-id
EDAMAM_APP_KEY=your-app-key
